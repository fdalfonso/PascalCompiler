# Macros
CND_PLATFORM=GNU-Linux-x86
CND_DLIB_EXT=so
CND_CONF=Debug
CND_DISTDIR=dist
CND_BUILDDIR=build

# Include project Makefile
include Makefile

# Object Directory
OBJECTDIR=${CND_BUILDDIR}/${CND_CONF}/${CND_PLATFORM}

# Object Files
OBJECTFILES= \
	${OBJECTDIR}/buffer.o \
	${OBJECTDIR}/common.o \
	${OBJECTDIR}/error.o \
	${OBJECTDIR}/list.o


# C Compiler Flags
CFLAGS=

# CC Compiler Flags
